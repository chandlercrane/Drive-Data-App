platform :ios, '12.0'

target 'DriveData' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for DriveData
	pod 'MapboxVisionNativeAll', :podspec => 'https://api.mapbox.com/downloads/v1/vision/ios/mapbox-vision-native-all/0.5.0.podspec?access_token=sk.eyJ1IjoiY2hhbmRsZXJjcmFuZSIsImEiOiJjanh1ajJkN3oxNXAwM3BxZmV6NHE2NHdsIn0.73g8pUefqHeyLeUzL3JRug'

	# Vision only
	pod "MapboxVision", :git => 'https://github.com/mapbox/mapbox-vision-ios.git', :tag => 'v0.5.0'

	# AR
	pod "MapboxVisionAR", :git => 'https://github.com/mapbox/mapbox-vision-ios.git', :tag => 'v0.5.0'

	# Safety
	pod "MapboxVisionSafety", :git => 'https://github.com/mapbox/mapbox-vision-ios.git', :tag => 'v0.5.0'
end
